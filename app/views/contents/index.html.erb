  <div class="wrapper">
    <div class="row">
      <div class="col-xs-12">
        <h1 class="name">
          My Contents
        </h1>
        <section class="inform">
          <h1>INFORM BACKET</h1>
          <div class="URL">
            Go to
            <a href="https://arcane-badlands-67342.herokuapp.com/">
              <div class="btn btn-default btn-sm">
                InformBacket
              </div>
            </a>
